<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" fillWidth="false" prefHeight="-1.0" prefWidth="-1.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controller.CustomerLayoutController">
  <!-- TODO Add Nodes -->
  <children>
    <HBox alignment="BOTTOM_LEFT" prefHeight="28.0" prefWidth="537.0" spacing="10.0">
      <children>
        <Button mnemonicParsing="false" onAction="#searchCustomers" text="Search All" />
        <Button mnemonicParsing="false" onAction="#searchCustomer" text="Search" />
        <TextField fx:id="customerSearchField" prefWidth="104.0" />
        <Button mnemonicParsing="false" onAction="#handleEdit" text="Edit" />
      </children>
    </HBox>
    <TableView fx:id="customerTable" maxWidth="-Infinity" prefHeight="200.0" prefWidth="527.0" VBox.vgrow="ALWAYS">
      <columns>
        <TableColumn minWidth="50.0" prefWidth="75.0" text="customerID" fx:id="customerIDColumn" />
        <TableColumn minWidth="50.0" prefWidth="150.0" text="customerEmail" fx:id="customerEmailColumn" />
        <TableColumn minWidth="50.0" prefWidth="150.0" text="customerPassword" fx:id="customerPasswordColumn" />
        <TableColumn minWidth="50.0" prefWidth="150.0" text="customerPhone" fx:id="customerPhoneColumn" />
      </columns>
      <VBox.margin>
        <Insets bottom="10.0" right="10.0" top="10.0" />
      </VBox.margin>
    </TableView>
  </children>
</VBox>
